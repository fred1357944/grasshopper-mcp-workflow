ghenv.Component.Name = "Wasp_Connection From Direction"
ghenv.Component.NickName = 'ConnDir'
ghenv.Component.Message = 'v0.6.001'
ghenv.Component.IconDisplayMode = ghenv.Component.IconDisplayMode.application
