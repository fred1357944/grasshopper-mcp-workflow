def main(part, transform):
    
    check_data = True
    
    ##check inputs
    if part is None:
        check_data = False
        msg = "Please provide a valid part to be transformed"
        ghenv.Component.AddRuntimeMessage(gh.Kernel.GH_RuntimeMessageLevel.Warning, msg)
    
    if transform is None:
