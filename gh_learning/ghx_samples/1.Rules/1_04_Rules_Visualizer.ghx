            aggregation.mode = aggregation_mode
            aggregation.reset_rules(aggregation.rules)
        
        #### constraints
        if global_constraints != aggregation.global_constraints:
            aggregation.global_constraints = global_constraints
            aggregation.reset_rules(aggregation.rules)
        
