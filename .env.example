# Grasshopper MCP Workflow Environment Variables
# Copy this file to .env and fill in your values
# NEVER commit .env to git!

# Gemini API Key (required for API mode)
# Get your key at: https://makersuite.google.com/app/apikey
GEMINI_API_KEY=your_gemini_api_key_here

# Optional: Anthropic API Key (for direct Claude API calls)
# ANTHROPIC_API_KEY=your_anthropic_key_here

# Grasshopper MCP Server
GRASSHOPPER_HOST=localhost
GRASSHOPPER_PORT=8080
